{
  "name": "@reatom/logger",
  "version": "3.8.1",
  "private": false,
  "sideEffects": false,
  "description": "Reatom for logger",
  "source": "src/index.ts",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.module.js"
  },
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "microbundle -f esm,cjs --css inline",
    "test": "tsc && tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "dependencies": {
    "@discoveryjs/discovery": "1.0.0-beta.83",
    "@reatom/core": "^3.8.1",
    "@reatom/jsx": "^3.11.0",
    "@reatom/lens": "^3.10.0"
  },
  "author": "artalar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/package/logger",
  "files": [
    "/build",
    "/package.json"
  ],
  "devDependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-react",
        {
          "pragma": "h",
          "pragmaFrag": "hf",
          "throwIfNamespace": false,
          "importSource": "@reatom/jsx"
        }
      ]
    ]
  }
}

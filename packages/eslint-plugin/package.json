{
  "name": "@reatom/eslint-plugin",
  "version": "3.8.0",
  "publishConfig": {
    "tag": "experimental"
  },
  "private": false,
  "sideEffects": false,
  "description": "Reatom-specific ESLint rules",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.mjs"
  },
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "browserslist": [
    "node 16"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "yoctobundle",
    "test": "tsc; tsx src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "author": "pivaszbs",
  "maintainers": [
    {
      "name": "krulod",
      "url": "https://github.com/krulod"
    },
    {
      "name": "artalar",
      "url": "https://github.com/artalar"
    },
    {
      "name": "pivaszbs",
      "url": "https://github.com/pivaszbs"
    },
    {
      "name": "serikovlearning",
      "url": "https://github.com/serikovlearning"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/package/eslint-plugin",
  "files": ["/build"],
  "devDependencies": {
    "@types/eslint": "^9.0.0",
    "eslint": "^9.21.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "dependencies": {
    "@types/estree": "^1.0.6"
  }
}

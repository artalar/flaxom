    "@reatom/effects": "^3.2.0",
    "@reatom/hooks": "^3.3.1",
    "@reatom/lens": "^3.3.2",
    "@reatom/logger": "^3.3.1",
    "@reatom/primitives": "^3.2.0",
    "@reatom/utils": "^3.2.0"
  },
  "author": "artalar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": { "url": "https://github.com/artalar/reatom/issues" },
  "homepage": "https://www.reatom.dev/packages/framework",
